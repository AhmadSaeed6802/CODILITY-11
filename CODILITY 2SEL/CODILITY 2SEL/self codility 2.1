        public int[] solution(int[] A, int K)
        {
            // Implement your solution here
            int[] B = (int[])A.Clone();
            for (int i = 0; i <= A.Length - 1; i++)
            {
                if (i + K <= A.Length - 1)
                {
                    A[i + K] = B[i];
                }
                else if (i + K > A.Length - 1)
                {
                    A[i + K - (A.Length - 1) - 1] = B[i];
                }
            }
            return A;
        }
